{
  "name": "nodeproto",
  "version": "0",
  "description": "full stack node starter kit for prototyping applications",
  "keywords": [
    "nirv",
    "nodeproto",
    "starter",
    "prototype",
    "fullstack",
    "api"
  ],
  "homepage": "https://github.com/noahehall/nodeproto",
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "noahehall (https://twitter.com/noahedwardhall",
  "main": "main.mjs",
  "type": "module",
  "config": {
    "APP_PORT": "3000",
    "NODE_ENV": "development"
  },
  "directories": {
    "bin": "./bin",
    "doc": "./doc"
  },
  "scripts": {
    "logenv": "npm run env",
    "about": "npm run-script && echo 'custom scripts: \n'; ls bin/*.sh",
    "prebuild:all": "rm -rf dist/*",
    "build:all": "node --experimental-json-modules -r dotenv/config lib/build.mjs",
    "start": "node -r dotenv/config main.mjs"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@tinyhttp/etag": "1.3.0",
    "dotenv": "10.0.0",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-charset": "2.0.0",
    "koa-compose": "4.1.0",
    "koa-csrf": "3.0.8",
    "koa-helmet": "6.1.0",
    "koa-jwt": "4.0.1",
    "koa-oas3": "2.3.0",
    "koa-passport": "4.1.4",
    "koa-qs": "3.0.0",
    "koa-ratelimit": "5.0.0",
    "koa-session": "6.2.0",
    "koa-sslify": "5.0.0",
    "koa-tree-router": "github:noahehall/koa-tree-router"
  },
  "devDependencies": {
    "esbuild": "0.12.4",
    "esbuild-plugin-manifest": "0.4.2",
    "husky": "6.0.0",
    "multi-tape": "1.6.1",
    "openapi-to-postmanv2": "2.7.0",
    "tap-spec": "2.2.2",
    "tape": "5.2.2",
    "tape-promise": "4.0.0"
  }
}
