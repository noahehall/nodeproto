{
  "name": "nodeproto",
  "version": "0",
  "description": "full stack node starter kit for prototyping applications",
  "keywords": [
    "nirv",
    "nodeproto",
    "starter",
    "prototype",
    "fullstack",
    "api",
    "koa",
    "openapi",
    "swagger"
  ],
  "homepage": "https://github.com/noahehall/nodeproto",
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "noahehall (https://twitter.com/noahedwardhall",
  "main": "main.mjs",
  "type": "module",
  "config": {
    "APP_PORT": "3000",
    "NODE_ENV": "development"
  },
  "directories": {
    "bin": "./bin",
    "doc": "./doc"
  },
  "scripts": {
    "hint": "hint http://localhost:3000",
    "test": "multi-tape {app,lib}/**/*.test.mjs | tap-spec",
    "about": "npm list && npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh",
    "predev": "rm -rf dist/*",
    "dev": "node --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules -r dotenv/config lib/esbuild.config.mjs",
    "start": "npm run dev",
    "start:native": "node --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules -r dotenv/config main.mjs"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@messageformat/react": "1.0.0",
    "@tinyhttp/etag": "1.3.0",
    "cls-rtracer": "2.6.0",
    "js-yaml": "3.14.1",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-bodyparser": "4.3.0",
    "koa-charset": "2.0.0",
    "koa-compose": "4.1.0",
    "koa-csrf": "3.0.8",
    "koa-helmet": "6.1.0",
    "koa-jwt": "4.0.1",
    "koa-oas3": "2.3.0",
    "koa-passport": "4.1.4",
    "koa-qs": "3.0.0",
    "koa-ratelimit": "5.0.0",
    "koa-session": "6.2.0",
    "koa-sslify": "5.0.0",
    "koa-tree-router": "github:noahehall/koa-tree-router",
    "modernizr": "3.11.7",
    "normalize.css": "8.0.1",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-throw-expressions": "7.12.13",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "babel-plugin-tailcall-optimization": "2.0.0",
    "core-js": "3.13.0",
    "create-hintrc": "3.0.11",
    "dotenv": "10.0.0",
    "esbuild": "0.12.4",
    "esbuild-plugin-manifest": "0.4.3",
    "hint": "6.1.3",
    "husky": "6.0.0",
    "jsdoc-to-markdown": "7.0.1",
    "multi-tape": "1.6.1",
    "sinon": "11.1.1",
    "tap-spec": "2.2.2",
    "tape": "5.2.2",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0"
  }
}
