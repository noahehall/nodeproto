{
  "name": "nodeproto",
  "version": "0",
  "description": "full stack node monorepo starter kit for prototyping applications",
  "keywords": [
    "nirv",
    "@nodeproto",
    "nodeproto",
    "starter",
    "prototype",
    "fullstack",
    "microservice",
    "micro service",
    "api",
    "koa",
    "openapi",
    "react",
    "webpack5",
    "esbuild",
    "docker",
    "swagger"
  ],
  "homepage": "https://github.com/noahehall/nodeproto",
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "noahehall (https://twitter.com/noahedwardhall",
  "directories": {
    "doc": "./doc",
    "packages": "./packages"
  },
  "scripts": {
    "about": "npm list && npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh",
    "lerna:about": "lerna run about",
    "lerna:about:help": "echo 'see available scripts & pkgs installed in each microservice'",
    "lerna:bootstrap": "lerna bootstrap --hoist --force-local",
    "lerna:bootstrap:help": "lerna bootstrap --help",
    "lerna:run:help": "lerna run --help",
    "lerna:start": "lerna run start --parallel",
    "lerna:start:help": "npm run lerna:run:help",
    "lerna:test": "lerna run test",
    "lerna:test:help": "npm run lerna:run:help",
    "lerna": "lerna",
    "lerna:help": "lerna --help"
  },
  "dependencies": {},
  "devDependencies": {
    "lerna": "4.0.0"
  }
}
