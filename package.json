{
  "author": "noahehall (https://twitter.com/noahedwardhall",
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "lerna monorepo for rapidly prototyping end-to-end, complex, and secure applications and microservices",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "nodeproto",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "prometheus",
    "prototype",
    "react",
    "rushstack",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "directories": {
    "apps": "./apps",
    "common": "./common",
    "libraries": "./libraries",
    "tools": "./tools"
  },
  "config": {
    "NODE_OPTIONS": "--trace-uncaught --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules --preserve-symlinks-main --preserve-symlinks"
  },
  "scripts": {
    "list": "rush list",
    "update": "rush update --recheck",
    "update:f": "rush update --full",
    "update:fp": "npm run update:f --purge",
    "select": "rush-select",
    "about": "npm list && npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh && lerna info"

  },
  "devDependencies": {},
  "dependencies": {
    "@telia/rush-select": "^0.12.0"
  }
}
