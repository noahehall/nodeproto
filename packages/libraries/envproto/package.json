{
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "@nodeproto/envproto - DRY environment management",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "./dist/index.cjs",
  "name": "@nodeproto/envproto",
  "packageManager": "pnpm@6.15.1",
  "private": true,
  "repository": "git://github.com/noahehall/nodeproto.git",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "email": "noahedwardhall+souls@gmail.com",
    "name": "theDRAGON",
    "url": "https://github.com/noahehall"
  },
  "config": {
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file somePkgJsonScriptName",
    "DIRS_TO_TRANSPILE": "./src",
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--enable-source-maps --heapsnapshot-near-heap-limit=3 --use-largepages=on --pending-deprecation --trace-sigint --trace-warnings --trace-exit --report-uncaught-exception --report-on-fatalerror --abort-on-uncaught-exception --experimental-specifier-resolution=node --experimental-json-modules --experimental-top-level-await --experimental-vm-modules --experimental-import-meta-resolve",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src"
  },
  "dependencies": {
    "@nodeproto/wtf": "workspace:*",
    "dotenv": "10.0.0",
    "env-cmd": "10.1.0",
    "pem": "1.14.4",
    "regenerator-runtime": "0.13.9",
    "ultra-runner": "3.10.5",
    "yargs": "17.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "2.17.0",
    "@nodeproto/configproto": "workspace:0.0.0",
    "@nodeproto/jsync": "workspace:*",
    "@nodeproto/testproto": "workspace:0.0.0",
    "@swc/cli": "0.1.46",
    "@swc/core": "1.2.75",
    "chokidar": "3.5.2",
    "chokidar-cli": "3.0.0",
    "eslint": "7.32.0",
    "ultra-runner": "3.10.5",
    "uvu": "0.5.1"
  },
  "directories": {
    "dist": "./dist",
    "src": "./src"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "exports": {
    "default": "./dist/index.js",
    "import": "./src/index.mjs"
  },
  "jsync": {
    "root": false
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "build": "pnpm repo:rm:dist && pnpm repo:cp:cjs && swc $npm_package_config_DIRS_TO_TRANSPILE -d $npm_package_config_PATH_DIST --config-file ./node_modules/@nodeproto/configproto/src/swc/node.swcrc --copy-files",
    "jsync": "jsync",
    "proto": "pnpm exec ultra -r",
    "proto:slice": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" pnpm exec ultra -r --filter",
    "repo:about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "repo:config:list": "pnpm config list --json",
    "repo:cp:browserslist": "rm -f .browserslistrc && cp ./node_modules/@nodeproto/configproto/src/browserslist/.browserslistrc .",
    "repo:cp:cjs": "rm -f ./dist/package.json && mkdir -p dist && cp ./node_modules/@nodeproto/configproto/src/node/cjs.json ./dist/package.json",
    "repo:cp:configproto": "pnpm repo:cp:flow; pnpm repo:cp:cjs; pnpm repo:cp:browserslist",
    "repo:cp:flow": "rm -f .flowconfig && cp ./node_modules/@nodeproto/configproto/src/flowtype/.flowconfig .",
    "repo:deps": "pnpm exec ultra --info",
    "repo:eslint": "eslint 'src/**/*'",
    "repo:eslint:fix": "pnpm repo:eslint -- --fix",
    "repo:flow": "pnpm exec flow",
    "repo:install": "pnpm -r exec pnpm install",
    "repo:monitor": "pnpm exec ultra --monitor",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:rm:dist": "rm -rf dist/*",
    "repo:rm:nodemodules": "rm -rf node_modules",
    "repo:test": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu ./src \"test.(m|c)?js\"",
    "repo:test:help": "uvu --help",
    "repo:test:ing": "pnpm repo:test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"pnpm repo:test\"",
    "repo:testing": "pnpm repo:test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"pnpm repo:test\"",
    "repo:upgrade": "pnpm upgrade -L"
  },
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  }
}
