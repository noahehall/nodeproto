{
  "name": "@nodeproto/envproto",
  "description": "@nodeproto/envproto - DRY environment management",
  "version": "0.0.0",
  "type": "module",
  "jsync": {
    "root": false
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib"
  },
  "config": {
    "NODE_ENV": "development",
    "NODE_OPTIONS": "--pending-deprecation --experimental-wasm-modules --trace-warnings --experimental-top-level-await --enable-source-maps --experimental-vm-modules --experimental-worker --experimental-import-meta-resolve --experimental-report --trace-exit --heapsnapshot-near-heap-limit=3 --trace-uncaught --deprecation --trace-deprecation --no-insecure-http-parser --report-uncaught-exception --use-largepages=on --warnings --experimental-abortcontroller --report-on-fatalerror --experimental-json-modules --trace-sigint --abort-on-uncaught-exception --es-module-specifier-resolution=node --experimental-specifier-resolution=node --experimental-modules",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src",
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file somePkgJsonScriptName"
  },
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./dist/index.cjs",
      "default": "./index.mjs"
    }
  },
  "dependencies": {
    "@nodeproto/wtf": "workspace:*",
    "dotenv": "10.0.0",
    "env-cmd": "10.1.0",
    "pem": "1.14.4",
    "ultra-runner": "3.10.5",
    "yargs": "17.1.1"
  },
  "devDependencies": {
    "@nodeproto/jsync": "workspace:*"
  },
  "scripts": {
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "ultra": "ultra",
    "upgrade": "pnpm upgrade -L",
    "nvm:latest": "nvm install node --reinstall-packages-from=current",
    "post:nvm:latest": "nvm alias default node"
  },
  "peerDependencies": {
    "@nodeproto/jsync": "workspace:*"
  },
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=16.9",
    "npm": ">=7.18.1",
    "pnpm": ">=6.9"
  },
  "repository": "git://github.com/noahehall/nodeproto.git"
}
