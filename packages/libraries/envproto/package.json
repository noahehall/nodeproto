{
  "name": "@nodeproto/envproto",
  "description": "@nodeproto/envproto - DRY environment management",
  "version": "0.0.0",
  "type": "module",
  "jsync": {
    "root": false
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib"
  },
  "config": {
    "NODE_ENV": "development",
    "NODE_OPTIONS": "--trace-exit --trace-sigint --experimental-specifier-resolution=node --trace-uncaught --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules --preserve-symlinks-main --preserve-symlinks"
  },
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./dist/index.cjs",
      "default": "./index.mjs"
    }
  },
  "dependencies": {
    "dotenv": "10.0.0",
    "env-cmd": "10.1.0",
    "pem": "1.14.4",
    "ultra-runner": "3.10.5",
    "@nodeproto/wtf": "workspace:*",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@nodeproto/jsync": "workspace:*"
  },
  "scripts": {
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "ultra": "ultra",
    "upgrade": "pnpm upgrade -L"
  },
  "peerDependencies": {
    "@nodeproto/jsync": "workspace:*"
  },
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=16.5",
    "npm": ">=7.18.1",
    "pnpm": ">=6.9"
  },
  "repository": "git://github.com/noahehall/nodeproto.git"
}
