{
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "@nodeproto/buildproto - reusable configs",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "./dist/index.js",
  "module": "./src/index.mjs",
  "name": "@nodeproto/buildproto",
  "packageManager": "pnpm@6.15.1",
  "private": true,
  "repository": "git://github.com/noahehall/nodeproto.git",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "email": "noahedwardhall+souls@gmail.com",
    "name": "theDRAGON",
    "url": "https://github.com/noahehall"
  },
  "config": {
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file somePkgJsonScriptName",
    "DIRS_TO_TRANSPILE": "./src",
    "FLOW_LOADER": "./node_modules/@nodeproto/configproto/src/node/loaders/flow.mjs",
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--experimental-loader=\"./node_modules/@nodeproto/configproto/src/node/loaders/flow.mjs\" --experimental-specifier-resolution=node --experimental-json-modules --experimental-top-level-await --experimental-vm-modules --experimental-import-meta-resolve node --enable-source-maps --heapsnapshot-near-heap-limit=3 --use-largepages=on --pending-deprecation --trace-sigint --trace-warnings --trace-exit --report-uncaught-exception --report-on-fatalerror --abort-on-uncaught-exception",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src",
    "REMOVED_BABEL_OPTIONS": "--copy-files",
    "REMOVED_NODE_OPTIONS": ""
  },
  "dependencies": {
    "@babel/core": "7.15.8",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@babel/plugin-transform-flow-strip-types": "7.14.5",
    "@babel/runtime": "7.15.4",
    "@babel/traverse": "7.15.4",
    "@emotion/styled": "11.3.0",
    "@nodeproto/configproto": "workspace:0.0.0",
    "@nodeproto/wtf": "workspace:0.0.0",
    "@swc/cli": "0.1.51",
    "@swc/core": "1.2.95",
    "@swc/helpers": "0.2.13",
    "babel-loader": "8.2.2",
    "browserslist": "4.17.3",
    "bundle-stats-webpack-plugin": "3.1.3",
    "copy-webpack-plugin": "9.0.1",
    "core-js": "3.18.2",
    "css-loader": "6.4.0",
    "esbuild": "0.13.5",
    "esbuild-loader": "2.16.0",
    "esbuild-plugin-manifest": "0.4.6",
    "flow-remove-types": "2.161.0",
    "html-webpack-plugin": "5.3.2",
    "http-terminator": "3.0.3",
    "koa": "2.13.3",
    "memfs": "3.3.0",
    "mini-svg-data-uri": "1.3.3",
    "react-devtools": "4.19.2",
    "regenerator-runtime": "0.13.9",
    "schema-utils": "3.1.1",
    "string-replace-loader": "3.0.3",
    "style-loader": "3.3.0",
    "ultra-runner": "3.10.5",
    "webpack": "5.58.1",
    "webpack-dev-middleware-2": "5.0.2",
    "webpack-hot-middleware-2": "2.25.2"
  },
  "devDependencies": {
    "@changesets/cli": "2.17.0",
    "@nodeproto/configproto": "workspace:0.0.0",
    "@nodeproto/jsync": "workspace:0.0.0",
    "@nodeproto/testproto": "workspace:0.0.0",
    "@octokit/core": "3.5.1",
    "chokidar-cli": "3.0.0",
    "eslint": "8.0.0",
    "flow-bin": "0.161.0",
    "flow-remove-types": "2.161.0",
    "flow-typed": "3.3.1",
    "react": "18.0.0-alpha-55d75005b-20211011",
    "react-dom": "18.0.0-alpha-55d75005b-20211011",
    "uvu": "0.5.2"
  },
  "directories": {
    "dist": "./dist",
    "flowtyped": "./flow-typed",
    "src": "./src"
  },
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "default": "./dist/index.js"
    },
    "./esbuild": {
      "import": "./src/esbuild/index.esbuild.config.mjs",
      "default": "./dist/esbuild/index.esbuild.config.js"
    },
    "./esbuild/*": {
      "import": "./src/esbuild/*.esbuild.config.mjs",
      "default": "./dist/esbuild/*.esbuild.config.js"
    },
    "./esbuild/plugin/*": {
      "import": "./src/esbuild/plugin/*.esbuild.plugin.mjs",
      "default": "./dist/esbuild/plugin/*.esbuild.plugin.js"
    },
    "./package.json": "./package.json",
    "./webpack": {
      "import": "./src/webpack/index.webpack.config.mjs",
      "default": "./dist/webpack/index.webpack.config.js"
    },
    "./webpack/*": {
      "import": "./src/webpack/*.webpack.config.mjs",
      "default": "./dist/webpack/*.webpack.config.js"
    },
    "./webpack/bff/*": {
      "import": "./src/webpack/bff/*.webpack.mjs",
      "default": "./dist/webpack/bff/*.webpack.js"
    },
    "./webpack/loader/*": "./src/webpack/loader/*.cjs"
  },
  "files": [
    "./.flowconfig"
  ],
  "jsync": {
    "root": false
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "build": "pnpm cjs:swc",
    "build:debug": "pnpm cjs:swc:debug",
    "cjs:babel": "clear; pnpm repo:rm:dist && NODE_OPTIONS=$npm_package_config_NODE_OPTIONS babel $npm_package_config_DIRS_TO_TRANSPILE -d $npm_package_config_PATH_DIST --out-file-extension .js --ignore 'src/**/*.test.mjs' --ignore src/webpack/test.compiler.mjs",
    "cjs:swc": "pnpm clean && NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" swc $npm_package_config_DIRS_TO_TRANSPILE -d $npm_package_config_PATH_DIST --config-file ./node_modules/@nodeproto/configproto/src/swc/node.swcrc --copy-files",
    "cjs:swc:debug": "pnpm cjs:swc -- --sync",
    "clean": "pnpm repo:rm:dist && pnpm repo:cp:cjs",
    "js:to:cjs": "for f in ./dist/**/*.js; do mv -- \"$f\" \"${f%.js}.cjs\"; done",
    "jsync": "jsync",
    "proto": "pnpm exec ultra -r",
    "proto:slice": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" pnpm exec ultra -r --filter",
    "repo:about": "npm run-script && npm --versions && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "repo:config:list": "pnpm config list --json",
    "repo:cp:browserslist": "rm -f .browserslistrc && cp ./node_modules/@nodeproto/configproto/src/browserslist/.browserslistrc .",
    "repo:cp:cjs": "rm -f ./dist/package.json && mkdir -p dist && cp ./node_modules/@nodeproto/configproto/src/node/cjs.json ./dist/package.json",
    "repo:cp:configproto": "pnpm repo:cp:flow; pnpm repo:cp:cjs; pnpm repo:cp:browserslist",
    "repo:cp:flow": "rm -f .flowconfig && cp ./node_modules/@nodeproto/configproto/src/flowtype/.flowconfig .",
    "repo:deps": "pnpm exec ultra --info",
    "repo:eslint": "eslint './**/*'",
    "repo:eslint:fix": "pnpm repo:eslint -- --fix",
    "repo:exec:list": "pnpm exec ultra --list",
    "repo:exec:list:g": "pnpm repo:exec:list | grep $*",
    "repo:exec:list:v": "npm run-script",
    "repo:flow": "pnpm exec flow",
    "repo:flow:status": "pnpm exec flow status",
    "repo:flowtyped:install": "pnpm exec flow-typed install",
    "repo:monitor": "pnpm exec ultra --monitor",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:rm:dist": "rm -rf dist/*",
    "repo:rm:nodemodules": "rm -rf node_modules",
    "repo:test": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu ./src \"test.(m|c)?js\"",
    "repo:test:file": "node --experimental-specifier-resolution=node --experimental-import-meta-resolve $*",
    "repo:test:help": "uvu --help",
    "repo:testing": "pnpm repo:test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"pnpm repo:test\"",
    "repo:upgrade": "pnpm upgrade -L"
  },
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  }
}
