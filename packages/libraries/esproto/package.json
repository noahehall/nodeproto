{
  "name": "@nodeproto/esproto",
  "description": "@nodeproto/esproto - the esbuild sushi-knife ",
  "version": "0.0.0",
  "type": "module",
  "jsync": {
    "root": false
  },
  "directories": {
    "dist": "./dist",
    "lib": "./lib"
  },
  "config": {
    "NODE_ENV": "development",
    "NODE_OPTIONS": "--trace-exit --trace-sigint --experimental-specifier-resolution=node --trace-uncaught --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules --preserve-symlinks-main --preserve-symlinks"
  },
  "main": "./dist/index.cjs",
  "exports": {
    "./index": "./index.mjs",
    ".": {
      "import": "./index.mjs",
      "require": "./dist/index.cjs",
      "default": "./index.mjs"
    }
  },
  "dependencies": {
    "@nodeproto/envproto": "workspace:*",
    "@nodeproto/wtf": "workspace:*",
    "esbuild": "0.12.16",
    "esbuild-plugin-manifest": "0.4.6",
    "ultra-runner": "3.10.5"
  },
  "devDependencies": {
    "@nodeproto/jsync": "workspace:*"
  },
  "scripts": {
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "build": "env-cmd node --conditions=build lib/esbuild.config.mjs",
    "buildtest": "NODE_OPTIONS='--pending-deprecation --experimental-wasm-modules --trace-warnings --experimental-top-level-await --enable-source-maps --experimental-vm-modules --experimental-worker --experimental-import-meta-resolve --experimental-report --trace-exit --heapsnapshot-near-heap-limit=3 --trace-uncaught --deprecation --trace-deprecation --no-insecure-http-parser --report-uncaught-exception --use-largepages=on --warnings --experimental-abortcontroller --report-on-fatalerror --experimental-json-modules --trace-sigint --abort-on-uncaught-exception --es-module-specifier-resolution=node --experimental-specifier-resolution=node --experimental-modules' node esbuild.config.mjs",
    "ultra": "ultra",
    "upgrade": "pnpm upgrade -L"
  },
  "peerDependencies": {
    "@nodeproto/jsync": "workspace:*"
  },
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=16.5",
    "npm": ">=7.18.1",
    "pnpm": ">=6.9"
  },
  "repository": "git://github.com/noahehall/nodeproto.git"
}
