{
  "name": "@nodeproto/configproto",
  "version": "0.0.0",
  "description": "@nodeproto/configproto - reusable configs",
  "directories": {
    "babel": "./babel",
    "browserslist": "./browserslist",
    "cjs": "./cjs",
    "esbuild": "./esbuild",
    "eslint": "./eslint",
    "swc": "./swc",
    "webpack": "./webpack"
  },
  "main": "./cjs/index.cjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "default": "./cjs/index.cjs"
    },
    "./babel/*": "./babel/*.babelrc",
    "./browserslist": "./browserslist/.browserslistrc",
    "./flowconfig": "./.flowconfig",
    "./package.json": "./package.json",
    "./esbuild/*": {
      "import": "./esbuild/*.esbuild.config.mjs",
      "default": "./cjs/*.esbuild.config.cjs"
    },
    "./eslint": "./eslint/base.eslintrc.yml",
    "./eslint/*": {
      "default": "./eslint/*.eslintrc.yml"
    },
    "./swc/*": "./swc/*.swcrc",
    "./webpack/loader/*": "./webpack/loader/*.cjs",
    "./webpack/bff/*": {
      "import": "./webpack/bff/*.webpack.mjs",
      "default": "./cjs/*.webpack.cjs"
    },
    "./webpack/*": {
      "import": "./webpack/*.webpack.config.mjs",
      "default": "./cjs/*.webpack.config.cjs"
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu . test.mjs -i node_modules",
    "test:ing": "watchlist . --ignore cjs -- pnpm test",
    "test:help": "uvu --help",
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "ultra": "ultra",
    "upgrade": "pnpm upgrade -L",
    "d": "rm -rf cjs/*",
    "cjs:babel": "pnpm d && NODE_OPTIONS=$npm_package_config_NODE_OPTIONS babel $npm_package_config_DIRS_TO_TRANSPILE -d cjs --out-file-extension .cjs --ignore '**/*.test.mjs' --ignore **/test.compiler.mjs",
    "cjs:swc": "pnpm d && NODE_OPTIONS=$npm_package_config_NODE_OPTIONS swc $npm_package_config_DIRS_TO_TRANSPILE -d cjs --config-file ./swc/node.swcrc --env-name='production' && pnpm js:to:cjs",
    "js:to:cjs": "for f in ./cjs/*.js; do mv -- \"$f\" \"${f%.js}.cjs\"; done",
    "cjs:swc:debug": "pnpm cjs:swc -- --sync",
    "bl:update": "echo 'doesnt work with pnpm'; exit 0; browserslist --update-db",
    "bl:check": "browserslist"
  },
  "jsync": {
    "root": false
  },
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
