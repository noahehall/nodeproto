  "engines": {
    "node": ">=16.5",
    "npm": ">=7.18.1",
    "pnpm": ">=6.9"
  },
  "config": {
    "PATH_DIST": "./dist",
    "PATH_SRC": ".",
    "NODE_ENV": "development",
    "TEST_FILES": "'**/*.test.mjs'",
    "DIRS_TO_TRANSPILE": "esbuild/*.mjs webpack/*.{c,m}js webpack/loader/*.{c,m}js webpack/bff/*.{c,m}js",
    "REMOVED_BABEL_OPTIONS": "--copy-files",
    "REMOVED_NODE_OPTIONS": "",
    "NODE_OPTIONS": "--es-module-specifier-resolution=node --experimental-specifier-resolution=node --experimental-wasm-modules --trace-warnings --experimental-top-level-await --enable-source-maps --experimental-vm-modules --experimental-worker --experimental-import-meta-resolve --experimental-report --trace-exit --heapsnapshot-near-heap-limit=3 --trace-uncaught --no-insecure-http-parser --report-uncaught-exception --use-largepages=on --experimental-abortcontroller --report-on-fatalerror --experimental-json-modules --trace-sigint --abort-on-uncaught-exception --experimental-modules"
  },
