{
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "repository": "git://github.com/noahehall/nodeproto.git",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "cloudnative",
    "development",
    "fullstack",
    "inception",
    "jsync",
    "micro service",
    "microservice",
    "noahedwardhall",
    "openapi",
    "product development framework",
    "production",
    "prototype",
    "secure",
    "theDRAGON",
    "virtual",
    "virtualization"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "config": {
    "DIRS_TO_TRANSPILE": "./src",
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--experimental-loader=\"./src/node/loaders/flow.mjs\" --experimental-specifier-resolution=node --experimental-json-modules --experimental-top-level-await --experimental-vm-modules --experimental-import-meta-resolve node --enable-source-maps --heapsnapshot-near-heap-limit=3 --use-largepages=on --pending-deprecation --trace-sigint --trace-warnings --trace-exit --report-uncaught-exception --report-on-fatalerror --abort-on-uncaught-exception",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src"
  },
  "name": "@nodeproto/configproto",
  "description": "static configuration files",
  "scripts": {
    "proto:bin": "pnpm -r exec $*",
    "proto:script": "pnpm -r $*",
    "proto:slice": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" pnpm exec ultra -r --filter",
    "proto": "pnpm exec ultra -r",
    "repo:about": "npm run-script && npm --versions && pnpm c list && echo \"NODE OPTIONS set to: $npm_package_config_NODE_OPTIONS\"",
    "repo:deps": "ultra --info",
    "repo:eslint:fix": "pnpm repo:eslint -- --fix",
    "repo:eslint": "eslint './**/*'",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:rm:dist": "rm -rf dist/*",
    "repo:rm:nodemodules": "rm -rf node_modules/*",
    "repo:scripts:v": "npm run-script",
    "repo:scripts": "ultra --list"
  },
  "directories": {
    "src": "./src",
    "flowtyped": "./flow-typed"
  },
  "exports": {
    "./babel/*": "./src/babel/*.babelrc",
    "./browserslist": "./src/browserslist/.browserslistrc",
    "./flowconfig": "./src/flowtype/.flowconfig",
    "./eslint": "./src/eslint/.eslintrc.yml",
    "./eslint/*": "./src/eslint/*.eslintrc.yml",
    "./fixtures/*": "./src/fixtures/*",
    "./node/*": "./src/node/*",
    "./swc": "./src/swc/node.swcrc",
    "./swc/*": "./src/swc/*.swcrc"
  },
  "dependencies": {
    "@babel/core": "7.15.8",
    "@babel/eslint-parser": "7.15.8",
    "@babel/eslint-plugin": "7.14.5",
    "@babel/plugin-proposal-throw-expressions": "7.14.5",
    "@babel/plugin-syntax-jsx": "7.14.5",
    "@babel/plugin-transform-flow-strip-types": "7.14.5",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-react-inline-elements": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@babel/runtime": "7.15.4",
    "@babel/traverse": "7.15.4",
    "@emotion/babel-plugin": "11.3.0",
    "babel-plugin-tailcall-optimization": "2.0.0",
    "babel-plugin-transform-export-default-name": "2.1.0",
    "browserslist": "4.17.4",
    "core-js": "3.18.3",
    "eslint-plugin-fb-flow": "0.0.4",
    "eslint-plugin-flowtype-errors": "4.4.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-yml": "0.10.1",
    "yaml-eslint-parser": "0.4.1"
  },
  "devDependencies": {
    "@octokit/core": "3.5.1",
    "flow-bin": "0.162.0",
    "flow-typed": "3.3.1"
  }
}
