// removes flowtypes, e.g. before running esbuild in webpack
// https://github.com/babel/babel/tree/main/packages/babel-preset-flow/src
// appears to be identical to @babel/preset-flow...
// generally you'd use this if your not extending from client/node/base.babelrc'
{
  "plugins": [
    [
      "@babel/plugin-transform-flow-strip-types", {
        "requireDirective": true,
        "allowDeclareFields": true,
        "all": false
      }
    ],
    "@babel/plugin-syntax-jsx"
  ]
}
