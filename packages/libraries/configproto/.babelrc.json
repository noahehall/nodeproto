{
  "assumptions": {
    "enumerableModuleMeta": true
  },
  "plugins": [
    ["@babel/plugin-transform-modules-commonjs", {
      "allowTopLevelThis": true,
      "strict": false,
      "importInterop": "babel",
      "lazy": true,
    }],
    "@babel/plugin-syntax-top-level-await"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false,
        "targets": {
          "esmodules": false,
          "node": true
        },
        "bugfixes": true,
        "spec": false,
        "modules": "commonjs",
        "ignoreBrowserslistConfig": true
      }
    ]
  ]
}
