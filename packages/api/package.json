{
  "name": "@nodeproto/api",
  "version": "0.0.0",
  "main": "./main.mjs",
  "type": "module",
  "config": {
    "API_PORT": "3000",
    "NODE_ENV": "development"
  },
  "directories": {
    "app": "./app",
    "bin": "./bin",
    "dist": "./dist",
    "lib": "./lib"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "hint": "hint http://localhost:3000",
    "test": "multi-tape {app,lib}/**/*.test.mjs | tap-spec",
    "about": "npm list && npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh",
    "predev": "rm -rf dist/*",
    "dev": "node --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules -r dotenv/config lib/esbuild.config.mjs",
    "start": "npm run dev",
    "start:native": "node --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules -r dotenv/config main.mjs"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@tinyhttp/etag": "1.3.0",
    "cls-rtracer": "2.6.0",
    "js-yaml": "3.14.1",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-bodyparser": "4.3.0",
    "koa-charset": "2.0.0",
    "koa-compose": "4.1.0",
    "koa-csrf": "3.0.8",
    "koa-helmet": "6.1.0",
    "koa-jwt": "4.0.1",
    "koa-oas3": "2.3.0",
    "koa-passport": "4.1.4",
    "koa-qs": "3.0.0",
    "koa-ratelimit": "5.0.0",
    "koa-session": "6.2.0",
    "koa-sslify": "5.0.0",
    "koa-tree-router": "github:noahehall/koa-tree-router"
  },
  "devDependencies": {
    "create-hintrc": "3.0.11",
    "dotenv": "10.0.0",
    "esbuild": "0.12.4",
    "esbuild-plugin-manifest": "0.4.3",
    "hint": "6.1.3",
    "husky": "6.0.0",
    "jsdoc-to-markdown": "7.0.1",
    "multi-tape": "1.6.1",
    "sinon": "11.1.1",
    "tap-spec": "2.2.2",
    "tape": "5.2.2"
  }
}
