{
  "name": "@nodeproto/client",
  "version": "0.0.0",
  "main": "./main.mjs",
  "type": "module",
  "config": {
    "CLIENT_PORT": "8080",
    "HTML_INDEX": "./app/index.html",
    "NODE_ENV": "development",
    "REACT_APP_FILE": "./app/app.mjs",
    "REACT_APP_ID": "root"
  },
  "directories": {
    "app": "./app",
    "dist": "./dist",
    "lib": "./lib"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "NODE_OPTIONS=\"--trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules -r dotenv/config\" webpack --config ./lib/webpack.dev.config.mjs",
    "prestart": "npm run prebuild",
    "start": "NODE_OPTIONS=\"--trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules -r dotenv/config\" ./node_modules/.bin/webpack serve --config ./lib/webpack.dev.config.mjs --mode development",
    "start:client": "npm start"
  },
  "dependencies": {
    "@messageformat/react": "1.0.0",
    "@reach/router": "1.3.4",
    "animate.css": "4.1.1",
    "clsx": "1.1.1",
    "history": "5.0.0",
    "koa": "2.13.1",
    "modernizr": "3.11.7",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-json-pretty": "2.2.0",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.14.2",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.2",
