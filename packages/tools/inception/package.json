{
  "name": "@nodeproto/inception",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "directories": {
    "bin": {
      "inception": "bin/inception.sh"
    }
  },
  "scripts": {
    "check": "node index.mjs --check",
    "run": "node index.mjs --run",
    "inception": "node index.mjs --inception",
    "test": "echo \"Error: no test specified\" && exit 1",
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "ultra": "ultra"
  },
  "dependencies": {
    "@nodeproto/jsync": "workspace:0.0.0",
    "@nodeproto/wtf": "workspace:0.0.0",
    "chalk": "4.1.1",
    "chokidar": "3.5.2",
    "ultra-runner": "3.10.5",
    "yargs": "16.2.0",
    "env-cmd": "10.1.0"
  },
  "jsync": {
    "root": false
  },
  "main": "index.mjs",
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "@nodeproto/inception - link package(s) anywhere on disk to any other pkg for local development",
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=16.5",
    "npm": ">=7.18.1",
    "pnpm": ">=6.9"
  },
  "config": {
    "NODE_OPTIONS": "--experimental-specifier-resolution=node --trace-uncaught --trace-warnings --experimental-import-meta-resolve --enable-source-maps --experimental-json-modules --preserve-symlinks-main --preserve-symlinks"
  },
  "repository": "git://github.com/noahehall/nodeproto.git"
}
