{
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "bleeding-edge repo for prototyping end-to-end, complex, and secure applications and microservices",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "./dist/index.cjs",
  "name": "@nodeproto/pkgcheck",
  "packageManager": "pnpm@6.15.1",
  "private": true,
  "repository": "git://github.com/noahehall/nodeproto.git",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "config": {
    "PKGCHECK_HTTP_PORT": "3000",
    "PKGCHECK_HTTPS_PORT": "4430",
    "NODE_ENV": "development",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src",
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file somePkgJsonScriptName",
    "NODE_OPTIONS": "--enable-source-maps --heapsnapshot-near-heap-limit=3 --use-largepages=on --pending-deprecation --trace-sigint --trace-warnings --trace-exit --report-uncaught-exception --report-on-fatalerror --abort-on-uncaught-exception --experimental-specifier-resolution=node --experimental-json-modules --experimental-top-level-await --experimental-vm-modules --experimental-import-meta-resolve"
  },
  "dependencies": {
    "@changesets/cli": "2.17.0",
    "@koa/cors": "3.1.0",
    "@nodeproto/envproto": "workspace:0.0.0",
    "@nodeproto/jsync": "workspace:0.0.0",
    "@nodeproto/wtf": "workspace:0.0.0",
    "@tinyhttp/etag": "1.3.0",
    "cls-rtracer": "2.6.0",
    "env-cmd": "10.1.0",
    "es-main": "1.0.2",
    "js-yaml": "3.14.1",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-bodyparser": "4.3.0",
    "koa-charset": "2.0.0",
    "koa-compose": "4.1.0",
    "koa-csrf": "3.0.8",
    "koa-helmet": "6.1.0",
    "koa-jwt": "4.0.1",
    "koa-oas3": "2.3.0",
    "koa-passport": "4.1.4",
    "koa-qs": "3.0.0",
    "koa-ratelimit": "5.0.0",
    "koa-session": "6.2.0",
    "koa-sslify": "5.0.0",
    "koa-tree-router": "github:noahehall/koa-tree-router",
    "ultra-runner": "3.10.5",
    "uvu": "0.5.1"
  },
  "devDependencies": {
    "@nodeproto/configproto": "workspace:0.0.0",
    "eslint": "~7.29.0"
  },
  "directories": {
    "src": "./src",
    "bin": "./bin",
    "dist": "./dist"
  },
  "jsync": {
    "root": false
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "build": "clear; pnpm repo:rm:dist && NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" node esbuild.build.config.mjs",
    "eslint:fix": "pnpm eslint -- --fix",
    "eslint": "eslint 'src/**/*'",
    "proto": "pnpm exec ultra -r --filter",
    "repo:about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "repo:config:list": "pnpm config list --json",
    "repo:cp:flow": "cp node_modules/@nodeproto/configproto/.flowconfig .",
    "repo:deps": "pnpm exec ultra --info",
    "repo:install": "pnpm -r exec pnpm install",
    "repo:monitor": "pnpm exec ultra --monitor",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:rm:dist": "rm -rf dist/*",
    "repo:rm:nodemodules": "rm -rf node_modules",
    "repo:upgrade": "pnpm upgrade -L",
    "start:native": "env-cmd node --conditions=development main.mjs",
    "start:pkgcheck": "env-cmd node --conditions=development lib/esbuild.config.mjs",
    "start": "pnpm start:pkgcheck",
    "test:help": "uvu --help",
    "test:ing": "pnpm test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"pnpm test\"",
    "test": "clear; NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu ./src test.mjs"
  },
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  }
}
