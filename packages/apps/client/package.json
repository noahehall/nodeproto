{
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "@nodeproto/client - super-modern frontend prototype",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "./index.mjs",
  "name": "@nodeproto/client",
  "packageManager": "pnpm@6.15.1",
  "private": true,
  "repository": "git://github.com/noahehall/nodeproto.git",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "config": {
    "CLIENT_PORT": "8080",
    "HTML_INDEX": "./app/index.html",
    "NODE_ENV": "development",
    "REACT_APP_FILE": "./app/app.mjs",
    "REACT_APP_ID": "root",
    "NODE_OPTIONS": "--enable-source-maps --heapsnapshot-near-heap-limit=3 --use-largepages=on --pending-deprecation --trace-sigint --trace-warnings --trace-exit --report-uncaught-exception --report-on-fatalerror --abort-on-uncaught-exception --experimental-specifier-resolution=node --experimental-json-modules --experimental-top-level-await --experimental-vm-modules --experimental-import-meta-resolve",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src",
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file somePkgJsonScriptName",
    "DIRS_TO_TRANSPILE": "./src"
  },
  "dependencies": {
    "@babel/runtime": "7.14.8",
    "@messageformat/react": "1.0.0",
    "@nodeproto/wtf": "workspace:0.0.0",
    "@reach/router": "github:noahehall/router",
    "animate.css": "4.1.1",
    "clsx": "1.1.1",
    "env-cmd": "10.1.0",
    "history": "5.0.0",
    "immutability-helper": "3.1.1",
    "milligram": "1.4.1",
    "modernizr": "3.11.7",
    "normalize.css": "8.0.1",
    "react": "18.0.0-alpha-edfe50510-20210823",
    "react-aria": "3.8.0",
    "react-dom": "18.0.0-alpha-419cc9c37-20210726",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "react-is": "17.0.2",
    "react-json-pretty": "2.2.0",
    "react-stately": "3.7.0",
    "reakit": "1.3.8",
    "styled-components": "5.3.0",
    "regenerator-runtime": "0.13.9",
    "ultra-runner": "3.10.5"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@changesets/cli": "2.17.0",
    "@nodeproto/configproto": "workspace:0.0.0",
    "@nodeproto/jsync": "workspace:*",
    "@nodeproto/tests": "workspace:0.0.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.13.2",
    "browsertime": "13.1.2",
    "concurrently": "6.2.0",
    "css-loader": "6.2.0",
    "eslint": "7.32.0",
    "flow-bin": "0.156.0",
    "flow-typed": "3.3.1",
    "hint": "6.1.4",
    "lighthouse": "8.1.0",
    "react-devtools": "4.14.0",
    "string-replace-loader": "3.0.3",
    "style-loader": "3.2.1",
    "ultra-runner": "3.10.5",
    "uvu": "0.5.1",
    "webpack": "5.46.0",
    "webpack-hot-middleware-2": "2.25.2",
    "@swc/cli": "0.1.46",
    "@swc/core": "1.2.75"
  },
  "directories": {
    "app": "./app",
    "dist": "./dist",
    "lib": "./lib"
  },
  "jsync": {
    "root": false
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "hint": "hint http://localhost:8080",
    "lighthouse": "lighthouse http://localhost:8080 --output-path ./doc/lighthouse/localhost.html",
    "browsertime": "browsertime http://localhost:8080",
    "start:dev": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" node webpack.server.mjs",
    "start:reactdevtools": "react-devtools",
    "start": "concurrently pnpm:start:dev pnpm:start:reactdevtools",
    "start:client": "pnpm start",
    "proto": "pnpm exec ultra -r --filter",
    "repo:about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "repo:config:list": "pnpm config list --json",
    "repo:cp:flow": "cp node_modules/@nodeproto/configproto/.flowconfig .",
    "repo:deps": "pnpm exec ultra --info",
    "repo:eslint:fix": "pnpm repo:eslint -- --fix",
    "repo:eslint": "eslint 'src/**/*'",
    "repo:install": "pnpm -r exec pnpm install",
    "repo:monitor": "pnpm exec ultra --monitor",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:rm:dist": "rm -rf dist/*",
    "repo:rm:nodemodules": "rm -rf node_modules",
    "repo:test:help": "uvu --help",
    "repo:test:ing": "pnpm repo:test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"pnpm repo:test\"",
    "repo:test": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu ./src test.mjs",
    "repo:upgrade": "pnpm upgrade -L"
  },
  "sideEffects": [
    "./app/components/**/*.js",
    "*.css"
  ],
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  }
}
