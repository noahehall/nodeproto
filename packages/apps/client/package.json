{
  "name": "@nodeproto/client",
  "version": "0.0.0",
  "main": "./index.mjs",
  "type": "module",
  "sideEffects": [
    "./app/components/**/*.js",
    "*.css"
  ],
  "config": {
    "CLIENT_PORT": "8080",
    "HTML_INDEX": "./app/index.html",
    "NODE_ENV": "development",
    "REACT_APP_FILE": "./app/app.mjs",
    "REACT_APP_ID": "root",
    "NODE_OPTIONS": "--pending-deprecation --experimental-wasm-modules --trace-warnings --experimental-top-level-await --enable-source-maps --experimental-vm-modules --experimental-worker --experimental-import-meta-resolve --experimental-report --trace-exit --heapsnapshot-near-heap-limit=3 --trace-uncaught --deprecation --trace-deprecation --no-insecure-http-parser --report-uncaught-exception --use-largepages=on --warnings --experimental-abortcontroller --report-on-fatalerror --experimental-json-modules --trace-sigint --abort-on-uncaught-exception --es-module-specifier-resolution=node --experimental-specifier-resolution=node --experimental-modules",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src",
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file somePkgJsonScriptName"
  },
  "directories": {
    "app": "./app",
    "dist": "./dist",
    "lib": "./lib"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "hint": "hint http://localhost:8080",
    "lighthouse": "lighthouse http://localhost:8080 --output-path ./doc/lighthouse/localhost.html",
    "browsertime": "browsertime http://localhost:8080",
    "start:reactdevtools": "react-devtools",
    "start": "concurrently npm:start:dev npm:start:reactdevtools",
    "start:client": "npm start",
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "config:list": "pnpm config list --json",
    "cp:flow": "cp node_modules/@nodeproto/configproto/.flowconfig .",
    "eslint:fix": "pnpm eslint -- --fix",
    "eslint": "eslint 'src/**/*'",
    "pkg:deps": "pnpm exec ultra --info",
    "test:help": "uvu --help",
    "test:ing": "clear; pnpm test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"clear; pnpm test\"",
    "test": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu ./src test.mjs",
    "upgrade": "pnpm upgrade -L"
  },
  "jsync": {
    "root": false
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.14.8",
    "@messageformat/react": "1.0.0",
    "@reach/router": "github:noahehall/router",
    "animate.css": "4.1.1",
    "clsx": "1.1.1",
    "core-js": "~3.15.2",
    "env-cmd": "10.1.0",
    "history": "5.0.0",
    "immutability-helper": "3.1.1",
    "koa": "2.13.1",
    "milligram": "1.4.1",
    "modernizr": "3.11.7",
    "normalize.css": "8.0.1",
    "react": "18.0.0-alpha-edfe50510-20210823",
    "react-aria": "3.8.0",
    "react-dom": "18.0.0-alpha-419cc9c37-20210726",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "react-is": "17.0.2",
    "react-json-pretty": "2.2.0",
    "react-stately": "3.7.0",
    "reakit": "1.3.8",
    "styled-components": "5.3.0",
    "ultra-runner": "3.10.5",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/eslint-parser": "7.14.7",
    "@babel/eslint-plugin": "7.14.5",
    "@babel/plugin-transform-modules-commonjs": "7.14.5",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-react-inline-elements": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-modules": "0.1.4",
    "@babel/preset-react": "7.14.5",
    "@babel/traverse": "7.14.8",
    "@changesets/cli": "2.17.0",
    "@nodeproto/jsync": "workspace:*",
    "@nodeproto/tests": "workspace:0.0.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.13.2",
    "babel-plugin-tailcall-optimization": "2.0.0",
    "browsertime": "13.1.2",
    "bundle-stats-webpack-plugin": "3.0.1",
    "concurrently": "6.2.0",
    "css-loader": "6.2.0",
    "eslint": "~7.31.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-flowtype": "5.8.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "flow-bin": "0.156.0",
    "flow-typed": "3.3.1",
    "hint": "6.1.4",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.1",
    "jsdoc-to-markdown": "7.0.1",
    "jsonfile": "6.1.0",
    "lighthouse": "8.1.0",
    "multi-tape": "1.6.1",
    "react-devtools": "4.14.0",
    "semver": "7.3.5",
    "sinon": "11.1.1",
    "string-replace-loader": "3.0.3",
    "style-loader": "3.2.1",
    "svg-url-loader": "7.1.1",
    "tap-spec": "5.0.0",
    "ultra-runner": "3.10.5",
    "url-loader": "4.1.1",
    "webpack": "5.46.0",
    "webpack-dev-middleware-2": "5.0.2",
    "webpack-hot-middleware-2": "2.25.2",
    "workbox-webpack-plugin": "6.1.5",
    "uvu": "0.5.1"
  },
  "author": {
    "name": "theDRAGON",
    "email": "noahedwardhall+souls@gmail.com",
    "url": "https://github.com/noahehall"
  },
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "@nodeproto/client - super-modern frontend prototype",
  "private": true,
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "repository": "git://github.com/noahehall/nodeproto.git",
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  },
  "packageManager": "pnpm@6.15.1"
}
