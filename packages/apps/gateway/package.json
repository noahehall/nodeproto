{
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "bleeding-edge repo for prototyping end-to-end, complex, and secure applications and microservices",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "@nodeproto/gateway",
  "packageManager": "pnpm@6.15.1",
  "private": true,
  "repository": "git://github.com/noahehall/nodeproto.git",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "email": "noahedwardhall+souls@gmail.com",
    "name": "theDRAGON",
    "url": "https://github.com/noahehall"
  },
  "config": {
    "GATEWAY_CHROOT_DIR": "./var/lib/haproxy",
    "GATEWAY_GROUP": "haproxy",
    "GATEWAY_STATS_SOCK": "./run/haproxy/admin.sock",
    "GATEWAY_USER": "haproxy",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src"
  },
  "dependencies": {
    "ultra-runner": "3.10.5"
  },
  "devDependencies": {
    "@changesets/cli": "2.17.0",
    "@nodeproto/jsync": "workspace:0.0.0"
  },
  "directories": {
    "src": "./src"
  },
  "files": [],
  "jsync": {
    "forceRootValues": [
      "author",
      "bugs",
      "funding",
      "homepage",
      "license",
      "repository"
    ],
    "ignoreRootValues": [
      "*"
    ],
    "root": false,
    "spreadRootValues": [
      "cpu",
      "keywords",
      "os",
      "publishConfig",
      "version"
    ]
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "nodeproto",
    "openapi",
    "pnpm",
    "prometheus",
    "prototype",
    "react",
    "ssl",
    "starter",
    "swagger",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "build": "",
    "jsync": "jsync",
    "linkandsetowner": "src/usr/bin/linkandsetowner.sh",
    "proto": "pnpm exec ultra -r",
    "proto:slice": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" pnpm exec ultra -r --filter",
    "repo:about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "repo:config:list": "pnpm config list --json",
    "repo:deps": "pnpm exec ultra --info",
    "repo:eslint": "eslint 'src/**/*'",
    "repo:eslint:fix": "pnpm repo:eslint -- --fix",
    "repo:install": "pnpm -r exec pnpm install",
    "repo:monitor": "pnpm exec ultra --monitor",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:upgrade": "pnpm upgrade -L",
    "start": "src/usr/bin/startdefault.sh",
    "start:client": "pnpm start",
    "start:gateway": "pnpm start",
    "start:pkgcheck": "pnpm start"
  },
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  }
}
