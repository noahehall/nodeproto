; @nodeproto: the ultimate flowconfig
; @todo: https://github.com/flow-typed/flow-typed
; @todo: https://github.com/flow-typed/flow-typed/wiki/Importing-And-Using-Type-Definitions
[ignore]
<PROJECT_ROOT>/browsertime*/.*
<PROJECT_ROOT>/bundle*/.*
<PROJECT_ROOT>/doc/.*
<PROJECT_ROOT>/hint*/.*
<PROJECT_ROOT>/dist/.*

[untyped]
.*/node_modules/*

[lints]
all=warn
untyped-import=off

[options]
all=false
babel_loose_array_spread=false
emoji=true
exact_by_default=false
experimental.const_params=true
include_warnings=false
lazy_mode=none
log.file=./.flowlog
max_header_tokens=3
module.file_ext=.mjs
module.file_ext=.cjs
module.file_ext=.js
module.file_ext=.json
module.ignore_non_literal_requires=true
module.system=node
module.system.node.main_field=exports
module.system.node.main_field=browser
module.system.node.main_field=main
module.system.node.resolve_dirname=node_modules
module.use_strict=false
munge_underscores=false
no_flowlib=false
react.runtime=automatic
sharedmemory.log_level=1
; server.max_workers=1 use default value instead
suppress_type=$FlowFixMe
suppress_type=$FlowTODO
; types_first=false
; esproposal.class_instance_fields=enable throws err
; traces=false
; suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe you can now automatically use $FlowFixMe, $FlowIssue, FlowExpectedError, and $FlowIgnore
; types_first=false throws err
; well_formed_exports=true
; esproposal.class_static_fields=true throws err
