{
  "bugs": "https://github.com/noahehall/nodeproto/issues",
  "description": "@nodeproto/tests - various monorepo-level tests to be used as reference for consumer integrations. exports a no-frills testrunner for node",
  "homepage": "https://github.com/noahehall/nodeproto",
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "@nodeproto/tests",
  "packageManager": "pnpm@6.24.2",
  "private": true,
  "repository": "git://github.com/noahehall/nodeproto.git",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "email": "noahedwardhall+souls@gmail.com",
    "name": "theDRAGON",
    "url": "https://github.com/noahehall"
  },
  "config": {
    "BABEL_ESM_LOADER": "--loader babel-register-esm",
    "BABEL_SHOW_CONFIG_FOR": "e.g. BABEL_SHOW_CONFIG_FOR=absolute/path/to/file as lead to some pkgjson script",
    "DEBUG_FILE": "copypasta/react/helloworld/index.jsx",
    "DIRS_TO_TRANSPILE": "./src",
    "FLOW_LOADER": "./node_modules/@nodeproto/configproto/src/node/loaders/flow.mjs",
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--abort-on-uncaught-exception --diagnostic-dir=\"/var/.nodeproto/diagnostics\" --enable-source-maps --experimental-abortcontroller --experimental-modules --experimental-import-meta-resolve --experimental-json-modules --experimental-loader=\"./node_modules/@nodeproto/configproto/src/node/loaders/flow.mjs\" --experimental-specifier-resolution=node --experimental-top-level-await --experimental-vm-modules --pending-deprecation --report-compact --report-dir=\"/var/.nodeproto/reports\" --report-filename=nodeprotolog --report-on-fatalerror --report-on-signal --report-signal=SIGUSR2 --report-uncaught-exception --trace-deprecation --trace-exit --trace-sigint --trace-uncaught --trace-warnings --unhandled-rejections=throw --use-largepages=on",
    "NODE_OPTIONS_REMOVED": " --preserve-symlinks --preserve-symlinks-main   --v8-pool-size=0 --zero-fill-buffers",
    "PATH_DIST": "./dist",
    "PATH_SRC": "./src"
  },
  "dependencies": {
    "@babel/runtime": "7.16.5",
    "html-webpack-plugin": "5.5.0",
    "regenerator-runtime": "0.13.9",
    "ultra-runner": "3.10.5"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@babel/eslint-plugin": "7.16.5",
    "@changesets/cli": "2.19.0",
    "@nodeproto/buildproto": "workspace:0.0.0",
    "@nodeproto/configproto": "workspace:0.0.0",
    "@nodeproto/envproto": "workspace:0.0.0",
    "@nodeproto/jsync": "workspace:0.0.0",
    "@nodeproto/testproto": "workspace:0.0.0",
    "@nodeproto/wtf": "workspace:0.0.0",
    "@octokit/core": "3.5.1",
    "@swc/cli": "0.1.55",
    "@swc/core": "1.2.122",
    "browserslist": "4.19.1",
    "chokidar-cli": "3.0.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-fb-flow": "0.0.4",
    "eslint-plugin-flowtype-errors": "4.5.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-yml": "0.12.0",
    "flow-bin": "0.168.0",
    "flow-remove-types": "2.168.0",
    "flow-typed": "3.5.0",
    "http-terminator": "3.0.4",
    "koa": "2.13.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-devtools": "4.22.1",
    "react-dom": "17.0.2",
    "uvu": "0.5.2",
    "webpack": "5.65.0",
    "yaml-eslint-parser": "0.5.0"
  },
  "directories": {
    "dist": "./dist"
  },
  "engines": {
    "node": "^14.17.0 || ^16.11.0 || >=17.1.0"
  },
  "exports": {
    ".": "./index.mjs",
    "./package.json": "./package.json"
  },
  "jsync": {
    "root": false
  },
  "keywords": [
    "@nirv",
    "@nodeproto",
    "api",
    "bodyguard",
    "cloudnative",
    "development",
    "docker",
    "esbuild",
    "fullstack",
    "grafana",
    "haproxy",
    "inception",
    "jsync",
    "koa",
    "micro service",
    "microservice",
    "nirv",
    "noahedwardhall",
    "nodeproto",
    "openapi",
    "pnpm",
    "product development framework",
    "production",
    "prometheus",
    "prototype",
    "react",
    "secure",
    "ssl",
    "starter",
    "swagger",
    "theDRAGON",
    "virtual",
    "virtualization",
    "webpack5"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "proto": "pnpm exec ultra -r",
    "proto:bin": "pnpm -r exec $*",
    "proto:script": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" pnpm -r $*",
    "proto:slice": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" pnpm exec ultra -r --filter",
    "repo:about": "npm run-script && npm --versions && pnpm -r config list --json t && echo \"NODE OPTIONS set to: $npm_package_config_NODE_OPTIONS\"",
    "repo:about:scripts": "npm run-script",
    "repo:cp:browserslist": "rm -f .browserslistrc && cp ./node_modules/@nodeproto/configproto/.browserslistrc .",
    "repo:cp:cjs": "rm -f ./dist/package.json && mkdir -p dist && cp ./node_modules/@nodeproto/configproto/src/node/cjs.json ./dist/package.json",
    "repo:cp:configproto": "pnpm repo:cp:flow; pnpm repo:cp:cjs; pnpm repo:cp:browserslist",
    "repo:cp:flow": "rm -f .flowconfig && cp ./node_modules/@nodeproto/configproto/.flowconfig .",
    "repo:deps": "ultra --info",
    "repo:deps:direct": "npm ls",
    "repo:eslint": "eslint './**/*' && pnpm repo:flow:coverage",
    "repo:eslint:fix": "pnpm repo:eslint -- --fix",
    "repo:flow:coverage": "pnpm flow batch-coverage src",
    "repo:flowtyped:install": "flow-typed install",
    "repo:jsync": "jsync",
    "repo:monitor": "ultra --monitor",
    "repo:nuke": "pnpm repo:rm:dist; pnpm repo:rm:nodemodules",
    "repo:rm:dist": "rm -rf dist/*",
    "repo:rm:nodemodules": "rm -rf node_modules/*",
    "repo:scripts": "ultra --list",
    "repo:scripts:v": "npm run-script",
    "repo:test": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" uvu ./src \"test.(m|c)?js\"",
    "repo:test:file": "NODE_OPTIONS=\"$npm_package_config_NODE_OPTIONS\" node $*",
    "repo:test:help": "uvu --help",
    "repo:testing": "pnpm repo:test; chokidar \"src/**/*(?:.test)?.(m|c)?js\" -c \"pnpm repo:test\"",
    "repo:update": "pnpm -r up --latest",
    "serve": "npx serve",
    "start:react": "concurrently pnpm:start:webpack:server pnpm:start:reactdevtools",
    "start:reactdevtools": "react-devtools",
    "start:webpack:server": "node --experimental-specifier-resolution=node integration/configproto/webpack.server.mjs",
    "test:buildproto:esbuild:build": "node --experimental-specifier-resolution=node --experimental-import-meta-resolve src/integration/buildproto/esbuild.build.config.mjs",
    "test:buildproto:esbuild:popcopy": "node --experimental-specifier-resolution=node --experimental-import-meta-resolve src/integration/buildproto/esbuild.popcopy.config.mjs",
    "test:buildproto:esbuild:run": "node --experimental-specifier-resolution=node --experimental-import-meta-resolve src/integration/buildproto/esbuild.run.config.mjs",
    "test:configproto:babel:copypasta": "babel copypasta -d dist/cjs/copypasta --copy-files --out-file-extension .cjs",
    "test:configproto:eslint": "eslint './**/*'",
    "test:configproto:eslint:debug": "BABEL_SHOW_CONFIG_FOR=$npm_package_config_DEBUG_FILE pnpm test:configproto:eslint -- --debug",
    "test:configproto:eslint:fix": "pnpm test:configproto:eslint -- --fix --ignore-pattern 'copypasta/**'",
    "test:configproto:export:import": "node --experimental-specifier-resolution=node ./integration/configproto/import.test.mjs",
    "test:configproto:webpack:react:dev:build": "node --experimental-specifier-resolution=node integration/configproto/webpack.react.dev.config.mjs",
    "test:configproto:webpack:react:esbuild:build": "node --experimental-specifier-resolution=node integration/configproto/webpack.react.esbuild.config.mjs",
    "test:focus": "clear; pnpm test:buildproto:esbuild:popcopy"
  },
  "ultra": {
    "concurrent": [
      "eslint",
      "test",
      "cp:flow"
    ]
  }
}
