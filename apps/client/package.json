{
  "name": "@nodeproto/client",
  "version": "0.0.0",
  "main": "./index.mjs",
  "type": "module",
  "sideEffects": [
    "./app/components/**/*.js",
    "*.css"
  ],
  "config": {
    "CLIENT_PORT": "8080",
    "HTML_INDEX": "./app/index.html",
    "NODE_ENV": "development",
    "REACT_APP_FILE": "./app/app.mjs",
    "REACT_APP_ID": "root"
  },
  "directories": {
    "app": "./app",
    "dist": "./dist",
    "lib": "./lib"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "flowtyped:i-force": "flow-typed install",
    "flowtyped:i": "flow-typed update-cache",
    "flowtyped:v": "flow-typed validate-defs",
    "flow": "flow",
    "flow:q": "flow status --quiet",
    "flow:dev": "flow batch-coverage --show-all --from local app/",
    "prepublish": "rushx build",
    "hint": "hint http://localhost:8080",
    "eslint": "eslint \"{app,bff,lib}/**/*.+(m|c)js\"",
    "eslint:fix": "npm run eslint -- --fix",
    "lighthouse": "lighthouse http://localhost:8080 --output-path ./doc/lighthouse/localhost.html",
    "browsertime": "browsertime http://localhost:8080",
    "build": "rm -rf dist/* && env-cmd node ./lib/webpack.build.mjs",
    "build:prod": "rm -rf dist/* && env-cmd node ./lib/webpack.build.mjs NODE_ENV=production",
    "start:reactdevtools": "react-devtools",
    "start:dev": "./node_modules/.bin/env-cmd node ./bff/server.mjs",
    "start": "concurrently npm:start:dev npm:start:reactdevtools",
    "start:client": "npm start"
  },
  "dependencies": {
    "dep1": "1.0.0",
    "@babel/runtime-corejs3": "7.14.8",
    "@messageformat/react": "1.0.0",
    "@nodeproto/utils": "workspace:*",
    "@opentelemetry/tracing": "0.23.0",
    "@reach/router": "https://github.com/noahehall/router",
    "animate.css": "4.1.1",
    "clsx": "1.1.1",
    "core-js": "3.15.2",
    "env-cmd": "10.1.0",
    "history": "5.0.0",
    "immutability-helper": "3.1.1",
    "koa": "2.13.1",
    "milligram": "1.4.1",
    "modernizr": "3.11.7",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-aria": "3.7.1",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "react-is": "17.0.2",
    "react-json-pretty": "2.2.0",
    "react-stately": "3.6.1",
    "reakit": "1.3.8",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/eslint-parser": "7.14.5",
    "@babel/eslint-plugin": "7.13.16",
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@babel/plugin-transform-react-constant-elements": "7.13.13",
    "@babel/plugin-transform-react-inline-elements": "7.12.13",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-modules": "0.1.4",
    "@babel/preset-react": "7.13.13",
    "@babel/traverse": "7.14.5",
    "@opentelemetry/api": "^1.0.0-rc.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-tailcall-optimization": "2.0.0",
    "browsertime": "12.9.1",
    "bundle-stats-webpack-plugin": "3.0.1",
    "concurrently": "6.2.0",
    "css-loader": "5.2.6",
    "eslint": "7.29",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "flow-bin": "0.154.0",
    "flow-typed": "3.3.1",
    "hint": "6.1.4",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "husky": "6.0.0",
    "jsdoc-to-markdown": "7.0.1",
    "jsonfile": "6.1.0",
    "lighthouse": "8.0.0",
    "multi-tape": "1.6.1",
    "react-devtools": "4.13.5",
    "semver": "7.3.5",
    "sinon": "11.1.1",
    "string-replace-loader": "3.0.2",
    "style-loader": "2.0.0",
    "svg-url-loader": "7.1.1",
    "tap-spec": "2.2.2",
    "url-loader": "4.1.1",
    "webpack": "5.38.1",
    "webpack-dev-middleware-2": "5.0.2",
    "webpack-hot-middleware-2": "2.25.2",
    "workbox-webpack-plugin": "6.1.5"
  }
}
