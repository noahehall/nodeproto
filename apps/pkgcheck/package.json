{
  "name": "@nodeproto/pkgcheck",
  "version": "0.0.0",
  "main": "./dist/index.cjs",
  "type": "module",
  "exports": {
    "./dist/index.cjs": "./dist/index.cjs",
    ".": {
      "import": "./index.mjs",
      "require": "./dist/index.cjs",
      "default": "./index.mjs"
    }
  },
  "config": {
    "PKGCHECK_HTTP_PORT": "3000",
    "PKGCHECK_HTTPS_PORT": "4430",
    "NODE_ENV": "development"
  },
  "directories": {
    "app": "./app",
    "bin": "./bin",
    "dist": "./dist",
    "lib": "./lib"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "prepublish": "rushx build",
    "eslint": "eslint \"{app,lib}/**/*.+(m|c)js\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "env-cmd multi-tape --conditions=test \"{app,lib}/**/*.test.mjs\" | tap-spec",
    "rm:dist": "rimraf dist",
    "prebuild": "npm run rm:dist",
    "build": "env-cmd node --conditions=build lib/esbuild.config.mjs",
    "prestart:pkgcheck": "npm run rm:dist",
    "start:pkgcheck": "env-cmd node --conditions=development lib/esbuild.config.mjs",
    "start:native": "env-cmd node --conditions=development main.mjs",
    "start": "npm run start:pkgcheck"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@tinyhttp/etag": "1.3.0",
    "cls-rtracer": "2.6.0",
    "es-main": "1.0.2",
    "js-yaml": "3.14.1",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-bodyparser": "4.3.0",
    "koa-charset": "2.0.0",
    "koa-compose": "4.1.0",
    "koa-csrf": "3.0.8",
    "koa-helmet": "6.1.0",
    "koa-jwt": "4.0.1",
    "koa-oas3": "2.3.1",
    "koa-passport": "4.1.4",
    "koa-qs": "3.0.0",
    "koa-ratelimit": "5.0.0",
    "koa-session": "6.2.0",
    "koa-sslify": "5.0.0",
    "koa-tree-router": "https://github.com/noahehall/koa-tree-router",
    "@nodeproto/utils": "workspace:*",
    "env-cmd": "10.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.5",
    "@babel/eslint-plugin": "7.13.16",
    "dotenv": "10.0.0",
    "esbuild": "0.12.8",
    "esbuild-plugin-manifest": "0.4.3",
    "eslint": "7.29",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "husky": "6.0.0",
    "jsdoc-to-markdown": "7.0.1",
    "multi-tape": "1.6.1",
    "sinon": "11.1.1",
    "tap-spec": "2.2.2",
    "tape": "5.2.2",
    "rimraf": "3.0.2",
    "@babel/core": "7.14.3"
  }
}
