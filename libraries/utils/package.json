{
  "name": "@nodeproto/utils",
  "version": "0.0.0",
  "type": "module",
  "directories": {
    "dist": "./dist",
    "fstest": "./fstest",
    "envproto": "./envproto",
    "esproto": "./esproto",
    "fsproto": "./fsproto",
    "lib": "./lib"
  },
  "config": {
    "NODE_ENV": "development"
  },
  "main": "./dist/index.cjs",
  "exports": {
    "./index": "./index.mjs",
    ".": {
      "import": "./index.mjs",
      "require": "./dist/index.cjs",
      "default": "./index.mjs"
    }
  },
  "dependencies": {
    "es-main": "1.0.2",
    "pem": "1.14.4",
    "mkdirp": "1.0.4",
    "env-cmd": "10.1.0",
    "dotenv": "10.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.5",
    "@babel/eslint-plugin": "7.13.16",
    "esbuild": "0.12.8",
    "esbuild-plugin-manifest": "0.4.3",
    "eslint": "7.29",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "rimraf": "3.0.2",
    "tap-spec": "2.2.2",
    "sinon": "11.1.1",
    "purple-tape": "3.9.2",
    "multi-tape": "1.6.1",
    "@babel/core": "7.14.3"
  },
  "scripts": {
    "about": "npm run-script && echo 'available via `bin/FILENAME`:\n'; ls bin/*.sh || exit 0",
    "eslint": "eslint \"{envproto,esproto,fsproto,lib}/**/*.+(m|c)js\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "env-cmd multi-tape --conditions=test  \"**/*.test.+(c|m)js\" | tap-spec",
    "prepublishOnly": "rimraf dist && npm run build",
    "prebuild": "rimraf dist",
    "build": "env-cmd node --conditions=build lib/esbuild.config.mjs"
  }
}
