#!/usr/bin/env sh

# because the verbosity of dry cmds sucks
# run any dry cmd via root/proto script

(type dry && type pnpm) > /dev/null;
test $? -ne 0 && (echo 'global installs required: dry-dry && pnpm'; exit 1)

echo "need to extract dry, then this file will become irrelevant"
exit 0

dry --dry-keep-package-json --dry-packager "pnpm" "$@"
